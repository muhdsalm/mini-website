{
  "name": "mini-website",
  "version": "2.0.1",
  "description": "Quickly launch an 11ty-generated static site. Includes a minimal Sass framework, generated sitemap, and RSS feed.",
  "main": "index.html",
  "scripts": {
    "start": "eleventy --serve & npx tailwindcss -i src/css/input.css -o src/css/output.css --watch",
    "build": "eleventy",
    "bump": "npm --no-git-tag-version version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muhdsalm/mini-website.git"
  },
  "keywords": [],
  "author": "5t3ph",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/muhdsalm/mini-website/issues"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "markdown-it": "^14.0.0",
    "markdown-it-anchor": "^8.6.7",
    "tailwindcss": "^3.4.1"
  },
  "browserslist": [
    "> 0.25%",
    "not dead"
  ],
  "dependencies": {
    "@11tyrocks/eleventy-plugin-sass-lightningcss": "^1.2.0",
    "bootstrap-icons": "^1.11.3"
  }
}
